[Unit]
Description=Kanata Keyboard Remapper
Documentation=https://github.com/jtroo/kanata

[Service]
Type=simple
# ExecStart=/usr/bin/kanata --cfg /home/Julien/.config/kanata/config.kbd
ExecStart=/home/julien/.cargo/bin/kanata --cfg /home/julien/.config/kanata/config.kbd
Restart=no

[Install]
WantedBy=default.target
